<odoo>
    <data noupdate="1">
        
        <!-- Customer Invoices -->
        <record id="seq_invoice" model="ir.sequence">
            <field name="name">Customer Invoices</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">INV/%(month)s%(y)s/</field>
        </record>


        <!-- Customer Invoices Refund -->
        <record id="seq_invoice_refund" model="ir.sequence">
            <field name="name">Customer Invoices Refund</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">CN/%(month)s%(y)s/</field>
        </record>


        <!-- Vendor Bills -->
        <record id="seq_vendor_bills" model="ir.sequence">
            <field name="name">Vendor Bills</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">BILL/%(month)s%(y)s/</field>
        </record>


        <!-- Vendor Bills Refund -->
        <record id="seq_vendor_bills_refund" model="ir.sequence">
            <field name="name">Vendor Bills Refund</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">DN/%(month)s%(y)s/</field>
        </record>


        <!-- Cash -->
        <record id="seq_cash" model="ir.sequence">
            <field name="name">Cash</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">CSH1/%(month)s%(y)s/</field>
        </record>


        <!-- Cash Basis Taxes -->
        <record id="seq_cash_basis_taxes" model="ir.sequence">
            <field name="name">Cash Basis Taxes</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">CABA/%(month)s%(y)s/</field>
        </record>


        <!-- Petty Cash -->
        <record id="seq_petty_cash" model="ir.sequence">
            <field name="name">Petty Cash</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">PCHS/%(month)s%(y)s/</field>
        </record>


        <!-- Point of Sale -->
        <record id="seq_point_of_sale" model="ir.sequence">
            <field name="name">Point of Sale</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">POSS/%(month)s%(y)s/</field>
        </record>


        <!-- BANK BCA -->
        <record id="seq_bank_bca" model="ir.sequence">
            <field name="name">BANK BCA</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">BCA1/%(month)s%(y)s/</field>
        </record>


        <!-- Exchange Difference -->
        <record id="seq_exchange_difference" model="ir.sequence">
            <field name="name">Exchange Difference</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">EXCH/%(month)s%(y)s/</field>
        </record>


        <!-- Inventory Valuation -->
        <record id="seq_inventory_valuation" model="ir.sequence">
            <field name="name">Inventory Valuation</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">STJ/%(month)s%(y)s/</field>
        </record>


        <!-- Miscellaneous Operations -->
        <record id="seq_miscellaneous_operations" model="ir.sequence">
            <field name="name">Miscellaneous Operations</field>
            <field name="code">account.move</field>
            <field name="padding">4</field>
            <field name="prefix">MISC/%(month)s%(y)s/</field>
        </record>

    </data>
</odoo>