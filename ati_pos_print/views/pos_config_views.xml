<?xml version="1.0"?>
<odoo>
    <record id="pos_config_form_inherit_ati_pos_print" model="ir.ui.view">
        <field name="name">pos.config.form.inherit.ati.pos.print</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='company']" position="after">
                <div id="apoteker" class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_right_pane">
                        <label for="employee_id"/>
                        <div><field name="employee_id" options="{'no_open': True, 'no_create': True}"/></div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>